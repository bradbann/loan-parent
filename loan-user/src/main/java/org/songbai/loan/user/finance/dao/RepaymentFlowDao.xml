<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.songbai.loan.user.finance.dao.RepaymentFlowDao">

    <select id="findFlowByOrderNumber" resultType="org.songbai.loan.model.loan.RepaymentFlowModel">
        select * from loan_a_repayment_flow where order_number=#{orderNumber} limit 1
