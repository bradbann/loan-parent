<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.songbai.loan.user.appConfig.dao.AppVersionDao">

    <select id="findByPlatform" resultType="org.songbai.loan.model.version.AppVersionModel">
        SELECT * FROM dream_v_version
        WHERE platform = #{platform} and agency_id = #{agencyId}
    </select>
</mapper>